		{
			"name": "Realizar novo cadastro de login com sucesso",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"function firstName (){\r",
							"    let firstName = [\"Ana\",\"Pedro\",\"Antonio\",\"Gustavo\",\"Paula\",\"Vanessa\",\"Carla\"];\r",
							"    let randomItem = firstName[Math.floor(Math.random() * firstName.length)];\r",
							"\r",
							"    return randomItem\r",
							"}\r",
							"\r",
							"pm.environment.set(\"FirstName\",firstName())\r",
							"//console.log(firstName())\r",
							"\r",
							"function lastName (){\r",
							"    let lastName = [\"Gomes\",\"Sampaio\",\"Souza\",\"Santos\",\"Oliveira\",\"Bezerra\",\"Ferreira\"];\r",
							"    let randomItem = lastName[Math.floor(Math.random() * lastName.length)];\r",
							"\r",
							"    return randomItem\r",
							"}\r",
							"\r",
							"pm.environment.set(\"LastName\",lastName());\r",
							"\r",
							"pm.environment.set(\"FullName\",pm.environment.get(\"FirstName\") + \" \" + pm.environment.get(\"LastName\"));\r",
							"\r",
							"function providerMail(){\r",
							"    let providerMail = [\"@yahoo.com\",\"@gmail.com\",\"@hotmail.com\",\"@uol.com\",];\r",
							"    let randomItem = providerMail[Math.floor(Math.random() * providerMail.length)];\r",
							"\r",
							"    return randomItem\r",
							"}\r",
							"pm.environment.set(\"ProviderMail\",providerMail());\r",
							"pm.environment.set(\"Email\",pm.environment.get(\"FirstName\")+ \"_\" +  pm.variables.replaceIn('{{$guid}}') + pm.environment.get(\"ProviderMail\"));\r",
							"\r",
							"function gerarCPF() {\r",
							"    var n = 9; \r",
							"    var n1 = Math.floor(Math.random() * 10);\r",
							"    var n2 = Math.floor(Math.random() * 10);\r",
							"    var n3 = Math.floor(Math.random() * 10);\r",
							"    var n4 = Math.floor(Math.random() * 10);\r",
							"    var n5 = Math.floor(Math.random() * 10);\r",
							"    var n6 = Math.floor(Math.random() * 10);\r",
							"    var n7 = Math.floor(Math.random() * 10);\r",
							"    var n8 = Math.floor(Math.random() * 10);\r",
							"    var n9 = Math.floor(Math.random() * 10);\r",
							"    var d1 = n9 * 2 + n8 * 3 + n7 * 4 + n6 * 5 + n5 * 6 + n4 * 7 + n3 * 8 + n2 * 9 + n1 * 10;\r",
							"    d1 = 11 - (d1 % 11);\r",
							"    if (d1 >= 10) d1 = 0;\r",
							"    var d2 = d1 * 2 + n9 * 3 + n8 * 4 + n7 * 5 + n6 * 6 + n5 * 7 + n4 * 8 + n3 * 9 + n2 * 10 + n1 * 11;\r",
							"    d2 = 11 - (d2 % 11);\r",
							"    if (d2 >= 10) d2 = 0;\r",
							"\r",
							"    return '' + n1 + n2 + n3  + n4 + n5 + n6 +  n7 + n8 + n9 + d1 + d2;\r",
							"}\r",
							"\r",
							"var cpf = gerarCPF();\r",
							"pm.environment.set(\"cpf_gerado\", cpf);\r",
							"\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
